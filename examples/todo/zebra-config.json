{
	"base": "./src",
	"output": "./static",
	"rules": {
		"js": {
			"compile": false,
			"uglify": false,
			"md5": {
				"exclude": []
			}
		},
		"css": {
			"compress": true,
			"md5": {
				"exclude": []
			}
		},
		"html": {
			"resourceMap": true
		},
		"image": {},
		"other": {
			"copy": true
		}
	},
	"clean": true
}